{
  "name": "dohdec",
  "version": "3.1.0",
  "description": "DNS over HTTPS and DNS over TLS",
  "main": "./lib/index.js",
  "scripts": {
    "lint": "standard bin/* lib/*.js test/*.js",
    "test": "ava",
    "coverage": "nyc -r lcov npm test",
    "doc": "jsdoc -c .jsdoc.conf",
    "precheck": "npm run lint && npm run test",
    "check": "npm pack --dry-run",
    "release": "npm version patch && git push --follow-tags && npm publish"
  },
  "keywords": [
    "doh",
    "dns-over-https",
    "dns-over-tls",
    "dns",
    "dnssec",
    "rfc7858",
    "rfc8484"
  ],
  "author": "Joe Hildebrand <joe-github@cursive.net>",
  "license": "MPL-2.0",
  "repository": "github:hildjj/dohdec",
  "bin": {
    "dohdec": "bin/dohdec"
  },
  "dependencies": {
    "dns-packet": "^5.3.0",
    "node-fetch": "^2.6.2",
    "nofilter": "^3.0.3",
    "yargs": "^17.1.1"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "jsdoc": "^3.6.7",
    "minami": "^1.2.3",
    "nock": "^13.1.3",
    "nyc": "^15.1.0",
    "standard": "^16.0.3"
  },
  "ava": {
    "files": [
      "test/*.ava.js"
    ]
  },
  "engines": {
    "node": ">=12.19"
  },
  "standard": {
    "ignore": [
      "/docs/**"
    ]
  }
}
