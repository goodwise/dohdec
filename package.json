{
  "name": "dohdec",
  "version": "3.1.0",
  "description": "DNS over HTTPS and DNS over TLS",
  "main": "./lib/index.js",
  "types": "types/",
  "type": "module",
  "scripts": {
    "lint": "eslint . --ext js",
    "test": "ava",
    "coverage": "c8 --all --src lib -r lcov npm test",
    "doc": "jsdoc -c .jsdoc.conf",
    "types": "tsc",
    "precheck": "npm run lint && npm run coverage && npm run types && npm run doc",
    "check": "npm pack --dry-run",
    "release": "npm version patch && git push --follow-tags && npm publish"
  },
  "keywords": [
    "doh",
    "dns-over-https",
    "dns-over-tls",
    "dns",
    "dnssec",
    "rfc7858",
    "rfc8484"
  ],
  "author": "Joe Hildebrand <joe-github@cursive.net>",
  "license": "MPL-2.0",
  "repository": "github:hildjj/dohdec",
  "bin": {
    "dohdec": "bin/dohdec.js"
  },
  "dependencies": {
    "dns-packet": "^5.3.0",
    "got": "^11.8.2",
    "nofilter": "^3.0.3",
    "yargs": "^17.1.1"
  },
  "devDependencies": {
    "@cto.af/eslint-config": "^0.0.8",
    "@types/dns-packet": "^5.2.2",
    "@types/node": "^16.9.2",
    "ava": "^3.15.0",
    "c8": "^7.9.0",
    "eslint": "^7.32.0",
    "eslint-plugin-ava": "^12.0.0",
    "eslint-plugin-jsdoc": "^36.1.0",
    "eslint-plugin-node": "^11.1.0",
    "jsdoc": "^3.6.7",
    "jsrsasign": "^10.4.0",
    "minami": "^1.2.3",
    "nock": "^13.1.3",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  }
}
